{
  "name": "server",
  "version": "1.0.0",
  "description": "ft-server",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node -r dotenv/config -r ts-node/register ./src/index.ts",
    "dev:watch": "nodemon",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "postinstall": "tsc",
    "deploy": "git push origin master && git push heroku master"
  },
  "engines": {
    "node": "12.16.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.5.0",
    "@types/cookie-parser": "^1.4.2",
    "bcrypt": "^5.0.0",
    "busboy-body-parser": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "into-stream": "^6.0.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.10",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.6",
    "sql-template-strings": "^2.2.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  }
}
